<?xml version="1.0" encoding="UTF-8" ?>
<!-- No copyright or license for configuration file, details here are not considered a creative work. -->
<moqui-conf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/moqui-conf-2.1.xsd">
    <default-property name="REMOTE_OFFICE_URL" value=""/>
    <tools>
        <tool-factory class="github.chunlinyao.jod.JodConverterToolFactory" init-priority="50" disabled="false"/>
    </tools>
    <resource-facade>
        <resource-reference scheme="jod+pdf+dbresource" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
        <resource-reference scheme="jod+pdf+http" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
        <resource-reference scheme="jod+pdf+https" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
        <resource-reference scheme="jod+pdf+file" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
        <resource-reference scheme="jod+pdf+jar" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
        <resource-reference scheme="jod+pdf+component" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
        <resource-reference scheme="jod+pdf+classpath" class="github.chunlinyao.jod.resource.ConvertToPdfResourceReference"/>
    </resource-facade>
</moqui-conf>
